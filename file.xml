<?xml version="1.0" encoding="UTF-8"?>
<uml:Model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:uml="http://www.omg.org/spec/UML/20131001">
  <uml:Class name="Customer">
    <uml:Attribute name="id" visibility="private" type="int"/>
    <uml:Attribute name="name" visibility="private" type="String"/>
    <uml:Attribute name="email" visibility="private" type="String"/>
    <uml:Operation name="Customer" visibility="public">
      <uml:Parameter name="id" direction="in" type="int"/>
      <uml:Parameter name="name" direction="in" type="String"/>
      <uml:Parameter name="email" direction="in" type="String"/>
    </uml:Operation>
    <uml:Operation name="getId" visibility="public" type="int"/>
    <uml:Operation name="getName" visibility="public" type="String"/>
    <uml:Operation name="getEmail" visibility="public" type="String"/>
    <uml:Operation name="setName" visibility="public">
      <uml:Parameter name="name" direction="in" type="String"/>
    </uml:Operation>
    <uml:Operation name="setEmail" visibility="public">
      <uml:Parameter name="email" direction="in" type="String"/>
    </uml:Operation>
  </uml:Class>
  <uml:Class name="Order">
    <uml:Attribute name="id" visibility="private" type="int"/>
    <uml:Attribute name="status" visibility="private" type="String"/>
    <uml:Attribute name="customer" visibility="private" type="Customer"/>
    <uml:Operation name="Order" visibility="public">
      <uml:Parameter name="id" direction="in" type="int"/>
      <uml:Parameter name="status" direction="in" type="String"/>
      <uml:Parameter name="customer" direction="in" type="Customer"/>
    </uml:Operation>
    <uml:Operation name="getId" visibility="public" type="int"/>
    <uml:Operation name="getStatus" visibility="public" type="String"/>
    <uml:Operation name="getCustomer" visibility="public" type="Customer"/>
    <uml:Operation name="setStatus" visibility="public">
      <uml:Parameter name="status" direction="in" type="String"/>
    </uml:Operation>
    <uml:Operation name="setCustomer" visibility="public">
      <uml:Parameter name="customer" direction="in" type="Customer"/>
    </uml:Operation>
  </uml:Class>
</uml:Model>
